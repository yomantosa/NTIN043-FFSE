#!/bin/bash

BASE=`dirname $0`
VERSION=3.0.2
NAME=commandlinetool

JAR=$BASE/${NAME}-${VERSION}-jar-with-dependencies.jar

if [ ! -f ${JAR} ]; then
    echo -e "Executable jar: \n\n${JAR}\n\ndoes not exist.  It must be in the same directory as this command ($0)."
    exit -1;
fi

java -jar ${JAR} $@


